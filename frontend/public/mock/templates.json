[
    {
        "id": "template-001",
        "name": "Standard NDA Template",
        "category": "legal",
        "description": "Mutual non-disclosure agreement for business discussions and potential partnerships",
        "version": "2.1",
        "lastUpdated": "2024-01-15T00:00:00Z",
        "previewUrl": "/mock/sample.pdf",
        "fields": [
            {
                "id": "field-001",
                "name": "disclosing_party",
                "label": "Disclosing Party Name",
                "type": "text",
                "required": true,
                "placeholder": "Enter company name"
            },
            {
                "id": "field-002",
                "name": "receiving_party",
                "label": "Receiving Party Name",
                "type": "text",
                "required": true,
                "placeholder": "Enter company name"
            },
            {
                "id": "field-003",
                "name": "effective_date",
                "label": "Effective Date",
                "type": "date",
                "required": true
            },
            {
                "id": "field-004",
                "name": "term_years",
                "label": "Term (Years)",
                "type": "number",
                "required": true,
                "defaultValue": 2,
                "validation": {
                    "min": 1,
                    "max": 10
                }
            },
            {
                "id": "field-005",
                "name": "governing_law",
                "label": "Governing Law",
                "type": "select",
                "required": true,
                "options": [
                    "California",
                    "New York",
                    "Delaware",
                    "Texas",
                    "Washington"
                ]
            },
            {
                "id": "field-006",
                "name": "purpose",
                "label": "Purpose of Disclosure",
                "type": "textarea",
                "required": true,
                "placeholder": "Describe the purpose..."
            }
        ]
    },
    {
        "id": "template-002",
        "name": "Employment Agreement - Tech",
        "category": "employment",
        "description": "Standard employment agreement for technical positions with IP assignment and non-compete clauses",
        "version": "3.0",
        "lastUpdated": "2024-02-01T00:00:00Z",
        "fields": [
            {
                "id": "field-007",
                "name": "employee_name",
                "label": "Employee Full Name",
                "type": "text",
                "required": true
            },
            {
                "id": "field-008",
                "name": "job_title",
                "label": "Job Title",
                "type": "text",
                "required": true,
                "placeholder": "e.g., Senior Software Engineer"
            },
            {
                "id": "field-009",
                "name": "start_date",
                "label": "Start Date",
                "type": "date",
                "required": true
            },
            {
                "id": "field-010",
                "name": "annual_salary",
                "label": "Annual Salary (USD)",
                "type": "number",
                "required": true,
                "validation": {
                    "min": 50000,
                    "max": 500000
                }
            },
            {
                "id": "field-011",
                "name": "work_location",
                "label": "Work Location",
                "type": "select",
                "required": true,
                "options": [
                    "Remote",
                    "San Francisco",
                    "New York",
                    "Austin",
                    "Seattle",
                    "Hybrid"
                ]
            },
            {
                "id": "field-012",
                "name": "benefits",
                "label": "Benefits Package",
                "type": "multiselect",
                "required": true,
                "options": [
                    "Health Insurance",
                    "401(k)",
                    "Stock Options",
                    "PTO",
                    "Remote Work"
                ]
            },
            {
                "id": "field-013",
                "name": "non_compete_months",
                "label": "Non-Compete Period (Months)",
                "type": "number",
                "required": false,
                "defaultValue": 12,
                "validation": {
                    "min": 0,
                    "max": 24
                }
            }
        ]
    },
    {
        "id": "template-003",
