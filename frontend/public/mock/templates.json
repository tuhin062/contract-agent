        "fields": [
            {
                "id": "field-014",
                "name": "client_name",
                "label": "Client Name",
                "type": "text",
                "required": true
            },
            {
                "id": "field-015",
                "name": "consultant_name",
                "label": "Consultant/Firm Name",
                "type": "text",
                "required": true
            },
            {
                "id": "field-016",
                "name": "project_scope",
                "label": "Project Scope",
                "type": "textarea",
                "required": true,
                "placeholder": "Describe the scope of work..."
            },
            {
                "id": "field-017",
                "name": "total_fee",
                "label": "Total Project Fee (USD)",
                "type": "number",
                "required": true
            },
            {
                "id": "field-018",
                "name": "payment_structure",
                "label": "Payment Structure",
                "type": "select",
                "required": true,
